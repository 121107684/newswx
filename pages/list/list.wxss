/* pages/list/list.wxss */
.indexlist{
  padding: 30rpx 40rpx 0
}
.indexpage{
  padding: 0 0 80rpx 50rpx;
  border-left: 1rpx solid #eaeaea;
  font-size: 32rpx;
  color: #a9a9a9;
  position: relative;
  line-height: 1.5
}
.indexpage .title {
  font-size: 32rpx;
  color: #000;
  font-weight: bold;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  padding: 48rpx 0 20rpx;
  line-height: 64rpx
}
.indexpage .time{
  position: absolute;
  top: 0;
  left:-22rpx;
  padding-left: 64rpx;
  color: #586b95;
  background: url(data:image/png;base64,) no-repeat left center
}
.footer {
  display: flex;
  padding-top:40rpx;
  justify-content:space-between
}
.footer .seeinfo, .footer .sharethis {
  width:180rpx;
  height:54rpx;
  background-color: #54c25d;
  color: #fff;
  border-radius: 27rpx;
  display: flex;
  justify-content:center;
  align-items: center
}
.footer .sharethis {
  background-color: transparent;
  color: #586b95;
  background: url(data:image/png;base64,) no-repeat left center ;
  padding-left: 30rpx
}
.sharebtnthis {
  position: absolute;
  top: 0;
  left: 0;
  width: 750rpx;
  opacity: 0;
}
